mysql –u root –p
CREATE USER ‘compsec’@’localhost’ IDENTIFIED by ‘compsec’;
GRANT ALL PRIVILEGES ON *.* TO ‘compsec’@’localhost’;
FLUSH PRIVILEGES;
exit
